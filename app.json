{
  "expo": {
    "name": "Trihp",
    "slug": "Trihp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "trihp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "This app uses your location to find nearby drivers and show your ride on the map.",
        "NSLocationAlwaysAndWhenInUseUsageDescription": "This app uses your location to find nearby drivers and show your ride on the map.",
        "NSLocationAlwaysUsageDescription": "This app uses your location to find nearby drivers and show your ride on the map."
      },
      "config": {
        "googleMapsApiKey": "AIzaSyDcw1UIsLcO8Gsp4X3txf0T8bmg1hiU5go"
      }
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/icon.png",
        // "backgroundImage": "./assets/images/android-icon-background.png",
        // "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.philzcodes.Trihp",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.INTERNET",
        "android.permission.ACCESS_NETWORK_STATE"
      ],
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDcw1UIsLcO8Gsp4X3txf0T8bmg1hiU5go"
        }
      }
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUseUsageDescription": "This app uses your location to find nearby drivers and show your ride on the map.",
          "locationWhenInUseUsageDescription": "This app uses your location to find nearby drivers and show your ride on the map."
        }
      ],
      [
        "expo-av",
        {
          "microphonePermission": false
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "targetSdkVersion": 35,
            "compileSdkVersion": 35
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "d2303d55-03ee-4c58-8ea8-e9619e518b5d"
      }
    }
  }
}
